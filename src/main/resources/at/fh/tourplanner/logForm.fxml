<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<GridPane
        xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="at.fh.tourplanner.controller.Log.LogsFormController"
>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="37.40000610351562" minHeight="10.0" prefHeight="37.40000610351562" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="61.400012207031246" minHeight="10.0" prefHeight="13.800012207031251" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="28.199987792968756" minHeight="10.0" prefHeight="28.199987792968756" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <VBox.margin>
        <Insets left="10.0" right="10.0" />
    </VBox.margin>
    <children>
        <Label text="Date/Time" />
        <Label text="Rating" GridPane.columnIndex="1">
            <GridPane.margin>
                <Insets left="5.0" />
            </GridPane.margin>
        </Label>
        <Label text="Difficulty" GridPane.columnIndex="2">
            <GridPane.margin>
                <Insets left="5.0" />
            </GridPane.margin>
        </Label>
        <Label text="Total Time" GridPane.columnIndex="3">
            <GridPane.margin>
                <Insets left="5.0" />
            </GridPane.margin>
        </Label>
        <TextField fx:id="durationTextField" GridPane.columnIndex="3" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="5.0" />
            </GridPane.margin>
        </TextField>
        <TextField fx:id="commentTextField" GridPane.columnSpan="3" GridPane.rowIndex="3" />
        <Label text="Comment" GridPane.rowIndex="2" />
        <DatePicker fx:id="datePickerField" GridPane.rowIndex="1" />
        <ChoiceBox  fx:id="difficultyTierChoiceBox" prefWidth="150.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="5.0" />
            </GridPane.margin>
        </ChoiceBox>
        <ChoiceBox fx:id="ratingChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="5.0" />
            </GridPane.margin>
        </ChoiceBox>
        <ButtonBar buttonMinWidth="20.0" prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="3">
            <buttons>
                <Button minWidth="25.0" mnemonicParsing="false" prefHeight="26.0" text="-" />
                <Button defaultButton="true" mnemonicParsing="false" text="+" />
            </buttons>
        </ButtonBar>
    </children>
    <padding>
        <Insets top="10.0" />
    </padding>
</GridPane>
