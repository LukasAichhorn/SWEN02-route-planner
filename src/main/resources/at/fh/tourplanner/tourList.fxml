<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml" fx:controller="at.fh.tourplanner.controller.Tour.TourListController"
     AnchorPane.bottomAnchor="0.0"
      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <ScrollPane prefHeight="474.0" prefWidth="247.0">
        <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="466.0" prefWidth="343.0">
                <children>
                    <ListView fx:id="tourList" layoutX="72.0" layoutY="50.0" prefHeight="200.0" prefWidth="200.0"
                              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                              AnchorPane.topAnchor="0.0"/>

                </children>
            </AnchorPane>
        </content>
        <VBox.margin>
            <Insets left="5.0" right="10.0" top="10.0"/>
        </VBox.margin>
    </ScrollPane>
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <Button defaultButton="true" mnemonicParsing="false" onAction="#openBlankFormButtonAction"
                fx:id="createButton" text="Create New" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets right="10.0"/>
            </GridPane.margin>
        </Button>
        <Button defaultButton="true" mnemonicParsing="false" onAction="#openFilledFormButtonAction"
                fx:id="editButton" text="Edit" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets right="10.0"/>
            </GridPane.margin>
        </Button>
    </GridPane>
</VBox>